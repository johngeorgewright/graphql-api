{
  "name": "@johngeorgewright/graphql-api",
  "version": "1.1.12",
  "description": "GraphQL API",
  "private": true,
  "main": "dist/entry.js",
  "scripts": {
    "build": "npm run clean && npm run gql && tsc",
    "clean": "rimraf '*(src/**/generated|dist)' || true",
    "env": "dotenv-extended --errorOnMissing=true --includeProcessEnv=true",
    "gql": "graphql-codegen",
    "migrate:up": "npm run env -- npm run orm migration:run",
    "migrate:down": "npm run env -- npm run orm migration:revert",
    "orm": "node --require ts-node/register node_modules/.bin/typeorm",
    "seed": "npm run env -- --path=.env.seed yarn orm migration:run",
    "unseed": "npm run env -- --path=.env.seed yarn orm migration:revert",
    "start": "concurrently --names gql,server --prefix-colors bgBlue.bold,bgMagenta.bold npm:start:*",
    "start:gql": "graphql-codegen --watch",
    "start:server": "npm run env -- ts-node-dev ./src/entry.ts",
    "test": "npm run env -- --path=.env.test jest --passWithNoTests",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/graphql-api.git"
  },
  "keywords": [],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/graphql-api/issues"
  },
  "homepage": "https://github.com/johngeorgewright/graphql-api#readme",
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@commitlint/types": "16.2.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/graphql-modules-preset": "2.3.10",
    "@graphql-codegen/typescript": "2.4.10",
    "@graphql-codegen/typescript-resolvers": "2.6.3",
    "@jest/types": "28.1.0",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.4",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "17.0.16",
    "@types/source-map-support": "0.5.4",
    "commitizen": "4.2.4",
    "concurrently": "7.1.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.0",
    "jest": "28.1.0",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "source-map-support": "0.5.21",
    "ts-jest": "28.0.2",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "npx prettier --ignore-path .gitignore --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "apollo-server": "3.7.0",
    "dataloader": "2.1.0",
    "dotenv-extended": "2.9.0",
    "express": "4.18.1",
    "graphql": "15.8.0",
    "graphql-import-node": "0.0.5",
    "graphql-modules": "2.0.0",
    "reflect-metadata": "0.1.13",
    "sqlite3": "5.0.8",
    "tslib": "2.4.0",
    "typeorm": "0.2.45"
  }
}
