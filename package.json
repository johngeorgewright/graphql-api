{
  "name": "@johngeorgewright/graphql-api",
  "version": "1.5.2",
  "description": "Compose and react upon redux actions",
  "private": true,
  "main": "dist/serve.js",
  "scripts": {
    "gql": "graphql-codegen",
    "orm": "node --require ts-node/register $(yarn bin typeorm)",
    "migrate:up": "yarn orm migration:run",
    "migrate:down": "yarn orm migration:revert",
    "seed": "yarn orm migration:run -c seed",
    "unseed": "yarn orm migration:revert -c seed",
    "start": "concurrently --names gql,server --prefix-colors bgBlue.bold,bgMagenta.bold 'yarn start:gql' 'yarn start:server'",
    "start:gql": "graphql-codegen --watch",
    "start:server": "ts-node-dev ./src/serve.ts",
    "test": "jest --passWithNoTests",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/graphql-api.git"
  },
  "keywords": [],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/graphql-api/issues"
  },
  "homepage": "https://github.com/johngeorgewright/graphql-api#readme",
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@graphql-codegen/cli": "2.5.0",
    "@graphql-codegen/graphql-modules-preset": "2.3.3",
    "@graphql-codegen/typescript": "2.4.3",
    "@graphql-codegen/typescript-resolvers": "2.5.0",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "9.0.0",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.16",
    "@types/source-map-support": "0.5.4",
    "commitizen": "4.2.4",
    "concurrently": "7.0.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "source-map-support": "0.5.21",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "yarn prettier --ignore-path .gitignore --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "apollo-server": "3.6.3",
    "dataloader": "2.0.0",
    "graphql": "16.3.0",
    "graphql-import-node": "0.0.4",
    "graphql-modules": "2.0.0",
    "reflect-metadata": "0.1.13",
    "sqlite3": "5.0.2",
    "tslib": "2.3.1",
    "typeorm": "0.2.41"
  },
  "packageManager": "yarn@3.1.1"
}